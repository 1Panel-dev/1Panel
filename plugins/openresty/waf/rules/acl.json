{
  "rules": [
    {
      "state": "on",
      "name": "no cookie",
      "conditions": [
        {
          "field": "URL",
          "pattern": "eq",
          "rule": "/test/\\d+\\.html"
        },
        {
          "field": "Cookie",
          "pattern": "eq",
          "rule": ""
        }
      ],
      "action": "deny",
      "code": 403,
      "res": "",
      "ipBlock": "off",
      "ipBlockTime": 60,
      "description": "拦截不带Cookie的请求"
    }
  ]
}